//break
i=1
while i<=5:
    print(i)
    if i==3:
        break
    i+=1
output:1 2 3
//continue
i=1
while i<=5:
    if (i==3):
        i+=1
        continue
    print(i)
    i+=1
output:1 2 4 5
//for 
tup=(1,5,8,9,7,6,4,3)
for num in tup:
    print(num)
else:
    print("end")
output:1 to 3 end
/////
tup=(4,5,6,7,8,9)
x=8
idx=0
for el in tup:
    if(el==x):
        print("num found",idx)
    idx+=1
